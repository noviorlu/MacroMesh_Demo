# GNU Make workspace makefile autogenerated by Premake

ifndef config
  config=debug
endif

ifndef verbose
  SILENT = @
endif

ifeq ($(config),debug)
  A3_config = debug
endif
ifeq ($(config),release)
  A3_config = release
endif

PROJECTS := A3

.PHONY: all clean help $(PROJECTS) 

all: $(PROJECTS)

A3:
ifneq (,$(A3_config))
	@echo "==== Building A3 ($(A3_config)) ===="
	@${MAKE} --no-print-directory -C build -f Makefile config=$(A3_config)
endif

clean:
	@${MAKE} --no-print-directory -C build -f Makefile clean

help:
	@echo "Usage: make [config=name] [target]"
	@echo ""
	@echo "CONFIGURATIONS:"
	@echo "  debug"
	@echo "  release"
	@echo ""
	@echo "TARGETS:"
	@echo "   all (default)"
	@echo "   clean"
	@echo "   A3"
	@echo ""
	@echo "For more information, see https://github.com/premake/premake-core/wiki"