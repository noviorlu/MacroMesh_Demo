# GNU Make workspace makefile autogenerated by Premake

ifndef config
  config=debug
endif

ifndef verbose
  SILENT = @
endif

ifeq ($(config),debug)
  cluster_config = debug
endif
ifeq ($(config),release)
  cluster_config = release
endif

PROJECTS := cluster

.PHONY: all clean help $(PROJECTS) 

all: $(PROJECTS)

cluster:
ifneq (,$(cluster_config))
	@echo "==== Building cluster ($(cluster_config)) ===="
	@${MAKE} --no-print-directory -C build -f Makefile config=$(cluster_config)
endif

clean:
	@${MAKE} --no-print-directory -C build -f Makefile clean

help:
	@echo "Usage: make [config=name] [target]"
	@echo ""
	@echo "CONFIGURATIONS:"
	@echo "  debug"
	@echo "  release"
	@echo ""
	@echo "TARGETS:"
	@echo "   all (default)"
	@echo "   clean"
	@echo "   cluster"
	@echo ""
	@echo "For more information, see https://github.com/premake/premake-core/wiki"